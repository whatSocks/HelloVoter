    "jmx": "github:OurVoiceUSA/node-jmx",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "logops": "^2.1.1",
    "migrate": "^1.7.0",
    "morgan-body": "^2.4.14",
    "neo4j-driver": "^1.7.6",
    "neode": "0.3.7",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "node-neo4j-bolt-adapter": "^1.0.6",
    "nodemailer": "^6.4.11",
    "nodemailer-smtp-transport": "^2.7.4",
    "papaparse": "^5.1.1",
    "patch-package": "^6.2.1",
    "plaid": "^5.1.0",
    "string-format": "^2.0.0",
    "stripe": "^8.63.0",
